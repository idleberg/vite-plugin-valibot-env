{
  "name": "vite-plugin-valibot-env",
  "version": "0.9.3",
  "description": "A Vite plugin to validate environment variables against a Valibot schema",
  "license": "MIT",
  "author": "Jan T. Sott",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/vite-plugin-valibot-env"
  },
  "keywords": [
    "vite-plugin",
    "valibot"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || >=22.0.0"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint:code": "eslint **/*.json ./src/**/*.ts --no-warn-ignored",
    "lint:deps": "knip --production",
    "lint": "concurrently --prefix '{name}' -c 'blue,green' 'npm:lint:*'",
    "prepack": "npm run build",
    "prepare": "husky",
    "publish:jsr": "deno publish",
    "publish:npm": "np --no-yarn --any-branch",
    "test": "uvu tests --require tsm --ignore \"fixtures\" --ignore \"helper.ts\""
  },
  "dependencies": {
    "is-unicode-supported": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.13.10",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "eslint": "9.22.0",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-unicorn": "^57.0.0",
    "husky": "^9.1.7",
    "knip": "^5.46.0",
    "nano-staged": "^0.8.0",
    "np": "^10.2.0",
    "prettier": "^3.5.3",
    "strip-ansi": "^7.1.0",
    "tsm": "^2.3.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "8.26.1",
    "uvu": "^0.5.6",
    "valibot": "1.0.0-rc.4",
    "vite": "^6.2.2"
  },
  "peerDependencies": {
    "valibot": ">=0.30 <2.0.0",
    "vite": ">=5 <7"
  },
  "nano-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --cache --write"
    ]
  },
  "packageManager": "pnpm@10.0.0+sha512.b8fef5494bd3fe4cbd4edabd0745df2ee5be3e4b0b8b08fa643aa3e4c6702ccc0f00d68fa8a8c9858a735a0032485a44990ed2810526c875e416f001b17df12b"
}
