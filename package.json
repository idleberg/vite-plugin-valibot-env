  "devDependencies": {
    "@lukeed/uuid": "^2.0.1",
    "@types/node": "^14.18.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.50.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "execa": "^8.0.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "np": "^10.0.5",
    "npm-run-all2": "^6.2.0",
    "strip-ansi": "^7.1.0",
    "tsm": "^2.3.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "uvu": "^0.5.6",
    "valibot": "^0.30.0",
    "vite": "^5.2.11"
  },
  "peerDependencies": {
    "valibot": "^0.30.0",
    "vite": "^4.0.0 || ^5.0.0"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "packageManager": "pnpm@9.1.2+sha512.127dc83b9ea10c32be65d22a8efb4a65fb952e8fefbdfded39bdc3c97efc32d31b48b00420df2c1187ace28c921c902f0cb5a134a4d032b8b5295cbfa2c681e2"
